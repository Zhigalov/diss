#!/usr/bin/gnuplot

set terminal "x11"

tf1=3.79473319220206e-07
a1=0.448980154404789
b1=-1.254536908460090
c1=2.07290719808643

tf2=4.89897948556635e-07
a2=0.346416294860588
b2=-0.923409768368471
c2=1.19087452707603

tf3=3.79473319220206e-07
a3=0.448
b3=-1.215
c3=1.616

f1(x)=a1+b1*x**2+c1*x**4
f2(x)=a2+b2*x**2+c2*x**4
f3(x)=a3+b3*x**2+c3*x**4

g1(x)=a1+b1*x**2
g2(x)=a2+b2*x**2
g3(x)=a3+b3*x**2

plot [0:0.3] \
 f1(x), f2(x), f3(x),\
 g1(x), g2(x), g3(x)
pause -1